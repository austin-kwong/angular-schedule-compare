                    $('#bdp_doc-viewer-image_start').waypoint(function (direction) {
                if (direction === 'down') {
                    $('.bdp_stickyHeader_container').addClass('visible').removeClass('invisible');
                }
                else {
                    $('.bdp_stickyHeader_container').addClass('invisible').removeClass('visible');
                }
            });

            $('.ch_docBottomActions_content').waypoint(function (direction) {
                if (direction === 'up') {
                    $('.bdp_stickyHeader_container').addClass('visible').removeClass('invisible');
                }
                else {
                    $('.bdp_stickyHeader_container').addClass('invisible').removeClass('visible');
                }
            }, {
                offset: '75%'
            });

            // set page num change to only occur after scroll from 1st page
            var pageScrolls = 0;
            $('.bdp_preview_single').waypoint(function (direction) {
                if (direction === 'up' && pageNum > 1) {
                    pageNum--;
                } else if (direction === 'down' && pageScrolls > 0 && pageNum < endPage) {
                    pageScrolls++;
                    pageNum++;
                } else if (direction === 'down' && pageScrolls < 1) {
                    pageScrolls = 1;
                }
                $('.greenText').text(pageNum);
            }, {
                offset: '75%'
            });
        
