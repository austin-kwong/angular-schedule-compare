    <script src="Repeat%20the%20question%20for%20b%20b%202%201%20B%20The%20matrix%20norm%20of%20a%20is%20the%20same%20as%20the%202_files/jquery.js"></script>
    <script type="text/javascript">
        var registerWayPoint = function(){
            jQuery("#ch-footer").waypoint(function(direction){
                if(direction == "up"){
                    $("#ch_askQuestionTray_wrapper").removeClass("isNotSticky");
                }
                else{
                    $("#ch_askQuestionTray_wrapper").addClass("isNotSticky");
                }
            }, { offset: "100%" });
        };

        var initAskQuestionTab = function(){
            registerWayPoint();

            if (typeof tracker === 'undefined') {
                var tracker = new MixpanelEventTracking();
            }

            jQuery("#ch_askQuestionTray_wrapper > .ch_askQuestionTray_tab").on("click", function() {
                $("#ch_askQuestionTray_wrapper").toggleClass("isOpen");
                // Mixpanel Tracking
                var properties = {};

                properties['url'] = window.location.href;
                properties['page_type'] = tracker.get_page_type();
                properties['action'] = $("#ch_askQuestionTray_wrapper").hasClass("isOpen") ? "open" : "close";
                tracker.track('[EXQW] Widget - Trigger', properties);
            });
        };

        jQuery(document).ready(function(){
            initAskQuestionTab();

            // dynamic pages need to recalculate viewport
            window.setInterval(function(){
                registerWayPoint();
            }, 300);
        });

    </script>
    
