        $("[data-ch-mixpanel-doc-thumb-tracking]").on('click', function (event) {
            var document_number = jQuery(event.delegateTarget).attr('data-ch-mixpanel-doc-thumb-tracking');
            var location = jQuery(event.delegateTarget).attr('data-ch-mixpanel-doc-location');
            if (document_number) {
                var properties = {};
                properties['Document Number'] = document_number;
                properties['Location'] = location;
                tracker.track("Doc Landing Related Content Clicks", properties);
            }
        });

