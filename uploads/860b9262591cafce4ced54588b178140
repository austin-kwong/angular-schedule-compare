    $(document).ready(function(){
        $('.bdp_sticky-footer_wrapper').waypoint(function() {
             $('.bdp_sticky-footer_container').toggleClass("stuck");
        },{
          offset: 'bottom-in-view'
        });

        $('.bdp_title_heading').waypoint(function(direction) {
            if (direction === 'down') {
                $('.bdp_sticky-footer_container').addClass('visible').removeClass('invisible');
            }
            else {
                $('.bdp_sticky-footer_container').addClass('invisible').removeClass('visible');
            }
        });

        $(".bdp_sticky-footer_action").click(function(e){
            _gaq.push(['_trackEvent', 'Doc Landing Page', 'Conversion Action', 'Sticky Footer Button']);

            // Mixpanel/Amplitude Tracking
            if(typeof tracker !== 'undefined') {
                tracker.track("Conversion Action", {'Conversion Name': 'Sticky Footer Button'});
            }
        });

    });
</script>

