        <script type="text/javascript" src="Repeat%20the%20question%20for%20b%20b%202%201%20B%20The%20matrix%20norm%20of%20a%20is%20the%20same%20as%20the%202_files/18200b0-e87cdee.js"></script>
        <script type="text/javascript">
        $(document).ready(function(){
            $(".bdp_course_info_wrapper_ab1 > .bdp_course_info_tab").on("click", function() {
                $(".bdp_course_info_wrapper_ab1").toggleClass("isOpen");
            });
            $(".bdp_course_info_wrapper_ab3 > .bdp_course_info_tab").on("click", function() {
                $(".bdp_course_info_wrapper_ab3 > .bdp_course_info_container").slideToggle(function(){
                    if($(".bdp_course_info_wrapper_ab3 > .bdp_course_info_tab").hasClass("isOpen")){
                        $(".bdp_course_info_wrapper_ab3 > .bdp_course_info_tab").removeClass("isOpen");   
                    } else{
                        $(".bdp_course_info_wrapper_ab3 > .bdp_course_info_tab").addClass("isOpen");
                    }        
                });
            });
        });
    </script>
    <script type="text/javascript">
    (function(){
        //function to check if the return key was pressed on mobile
        function checkKeyPress(classA){
            $(classA).keypress(function(event){
                var keycode = (event.keyCode ? event.keyCode : event.which);
                if(keycode == '13'){
                    $(classA).css("padding-right","100px");
                }
            });
        }
        //function to check if the return key was pressed on desktop
        function checkKeyPress_screen(classA, classB){
            $(classA).keypress(function(event){
                var keycode = (event.keyCode ? event.keyCode : event.which);
                if(keycode == '13'){
                    $(classB).hide();
                    $(classA).css("padding-right","100px");
                }
            });
        }
        function checkKeyPress_screen_nopadding(classA, classB){
            $(classA).keypress(function(event){
                var keycode = (event.keyCode ? event.keyCode : event.which);
                if(keycode == '13'){
                    $(classB).hide();
                }
            });
        }
        //function to check if we have the ng-hide class on the search count on mobile
        function checkClass(classA){
            if($("#search_count").hasClass("ng-hide")){
                $(classA).css("padding-right","12px");
                checkKeyPress(classA);
            } else{
                $(classA).css("padding-right","100px");
            }
        }
        //function to check if we have the ng-hide class on the search count on screen
        function checkClass_screen(classA){
            if($("#search_count").hasClass("ng-hide")){
                $(classA).css("padding-right","0px");
                checkKeyPress(classA);
            } else{
                $(classA).css("padding-right","100px");
            }
        }
        function checkClass_delay(classA){
            if($("#search_count").hasClass("ng-hide")){
                $(classA).css("padding-right","12px");
                checkKeyPress(classA);
            } else{
                setTimeout(function(){
                    $(classA).css("padding-right","100px");
                }, 300 );
            }
        }
        //function to check if any change has been made to the input field
        function checkInputChange(classA){
            $(classA).on('input', function(){
              if($(this).val() == ""){
                  $(classA).css("padding-right","12px");
              }
            });
        }
        //function to check if the enter tooltip is shown
        function checkTooltip(classA){
            setTimeout(function(){
                if($("#search_locked_tooltip_1").css("display","inline") && ($(".dp_doc-viewer_notification_1").hasClass("isExpanded"))){
                    $('.search_locked_tooltip_1').show();
                    $(classA).css("padding-right","100px");
                } else if($("#search_locked_tooltip_1").css("display","none")){
                    checkClass_screen(classA)
                }            
            }, 300);
        }
        //the main function to be called in document ready and also when window is resized
        function stuffForResizeAndReady(){
            if($(window).width() <= 1057){
                $('.dp_search_input_1,.dp_search_input_2,.dp_search_input_3').on('focus', function () {
                    checkClass('.dp_search_input_1,.dp_search_input_2, .dp_search_input_3');
                    checkInputChange('.dp_search_input_1,.dp_search_input_2, .dp_search_input_3');
                });
                $('.dp_search_input_1,.dp_search_input_2,.dp_search_input_3').on('focusout', function () {
                     checkClass('.dp_search_input_1,.dp_search_input_2, .dp_search_input_3');
                     $(".dp_search_input_1").css("padding-right","0px");
                });
            }
           if($(window).width() >= 1057){
            //bucket 1
                $('.dp_search_bar_1').on('click', function(e){
                    $(".bdp_notification_text").css("display","none");
                    $(".dp_doc-viewer_notification_1").addClass("isExpanded");
                    $('#dp_search_input_1').focus();
                    if($(".dp_search_input_1").val() == ""){
                        checkTooltip(".dp_search_input_1");
                        checkKeyPress_screen('.dp_search_input_1','.search_locked_tooltip_1');
                        checkInputChange('.dp_search_input_1');
                    }else{
                        $('.search_locked_tooltip_1').hide();
                        checkClass_delay(".dp_search_input_1");
                        checkKeyPress_screen_nopadding('.dp_search_input_1','.search_locked_tooltip_1');
                        checkInputChange('.dp_search_input_1');
                    }
                    e.stopPropagation();
                });
                $('.dp_search_input_1').on('focusout', function (e) {
                    $('.search_locked_tooltip_1').hide();
                    setTimeout(function(){
                        $(".bdp_notification_text").css("display","inline-block");    
                    }, 20);  
                    $(".dp_doc-viewer_notification_1").removeClass("isExpanded");
                    $('.dp_search_input_1').css("padding-right","0px");
                    e.stopPropagation();
                });
                //bucket 2 & 3
                $('.dp_search_input_2, .dp_search_input_3').on('focus', function () {
                    if($(this).val() == ""){
                        $('.search_locked_tooltip_2').show();
                        checkClass('.dp_search_input_2, .dp_search_input_3');
                    }
                    checkKeyPress_screen('.dp_search_input_2, .dp_search_input_3', '.search_locked_tooltip_2');
                    checkInputChange('.dp_search_input_2, .dp_search_input_3');
                });
                $('.dp_search_input_2, .dp_search_input_3').on('focusout', function () {
                    $('.search_locked_tooltip_2').hide();
                    $('.search_locked_tooltip_1').hide();   
                    checkClass('.dp_search_input_2, .dp_search_input_3');
                });
            }
        }
        $(window).on("resize", stuffForResizeAndReady);
        $(document).on("ready", stuffForResizeAndReady);
    })();
    </script>
