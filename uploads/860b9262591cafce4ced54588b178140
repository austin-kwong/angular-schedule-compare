<script>
    var currentUrl = window.location.href;
    var shareOnFacebook = function() {
        FB.init({
            appId   : window.facebookAppId,
            status  : true,
            cookie  : true,
            oauth   : true,
            version : 'v2.5'
        });

        FB.ui({
            method: 'share',
            href: currentUrl,
        }, function(response){});
    }

    var shareOnTwitter = function() {
        window.open("https://twitter.com/intent/tweet?url=" + encodeURIComponent(currentUrl));
    }

    var shareThroughEmail = function() {
        window.open("mailto:?to=&subject=" + encodeURIComponent("Check out this awesome document from Course Hero!") + "&body=" + encodeURIComponent(currentUrl));
    }

    // Share URL on logged out/basic pages
    $(".doc_shareUrl_container").click(function(e) {
        $("#js-referral-link").val(currentUrl);
        $("#js-referral-link").select();
        document.execCommand("copy");
        $(".doc_shareUrl_dialog").toggle();
        e.preventDefault();
    });


</script>
                    
